    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "obsidian": "^1.5.7-1",
    "obsidian-dataview": "^0.5.55",
    "ts-jest": "^29.1.4",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
